<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.sys.dao.CodeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.sys.entity.CodeEntity" id="codeMap">
        <result property="codeId" column="code_id"/>
        <result property="advertisersId" column="advertisers_id"/>
        <result property="sellerId" column="seller_id"/>
        <result property="activityId" column="activity_id"/>
        <result property="activityType" column="activity_type"/>
        <result property="isFocus" column="is_focus"/>
        <result property="isQr" column="is_qr"/>
        <result property="codeUser" column="code_user"/>
        <result property="status" column="status"/>
        <result property="region" column="region"/>
        <result property="addressDetail" column="address_detail"/>
    </resultMap>
    <insert id="insertCodeEntity" keyProperty="codeId" parameterType="io.renren.modules.sys.entity.CodeEntity" useGeneratedKeys="true">
        insert into tb_code ( code_id, advertisers_id, seller_id, activity_id, activity_type, is_focus, is_qr, code_user, status, region, address_detail)values ( #{codeId}, #{advertisersId}, #{sellerId}, #{activityId}, #{activityType}, #{isFocus}, #{isQr}, #{codeUser}, #{status}, #{region}, #{addressDetail})
    </insert>

</mapper>